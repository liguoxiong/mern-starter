{
  "name": "nodeexpress",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "@babel/node": "^7.6.2",
    "@babel/polyfill": "^7.6.0",
    "@babel/runtime": "^7.6.0",
    "bcrypt": "^3.0.6",
